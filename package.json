{
  "name": "telegram-bot",
  "version": "1.0.0",
  "description": "Telegram Bot Starter Kit",
  "keywords": [
    "bot",
    "telegram",
    "telegram-bot",
    "starter-kit"
  ],
  "author": "Agung Dirgantara <agungmasda29@gmail.com>",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build": "swc -s --strip-leading-paths -d dist src/",
    "start": "node dist/main.js",
    "start:dev": "concurrently \"npm run build -- --watch\" \"nodemon dist/main.js --delay 1\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ],
    "*.{json,yaml}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "@telegraf/session": "^2.0.0-beta.7",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "html-entities": "^2.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "redis": "^4.7.1",
    "telegraf": "^4.16.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@swc/cli": "^0.7.5",
    "@swc/core": "^1.11.24",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.17",
    "concurrently": "^9.1.2",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "nodemon": "^3.1.10",
    "prettier": "3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  }
}
